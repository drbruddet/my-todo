TealColor 			= #39cccc
GreyColor 			= #e0e1e2
DarkGreyColor 	= #333333

header.header
	background: DarkGreyColor
	margin-bottom: 60px
	padding: 40px
	text-align: center
	color: #ffffff
	.hide-completed label
		color: TealColor !important

.task
	&.checked
		text-decoration: line-through
		background: GreyColor
	.private-lock
		color: red
	.public-lock
		color: white
	.buttoncheck
		background: GreyColor !important

